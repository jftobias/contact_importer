<div class="container">
  <div class="media-body">
    <h1>Imported Files</h1>

    <table class="table table-striped">
      <thead>
        <tr>
          <th>File name </th>
          <th>Aasm state</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @imported_files.each do |file| %>
          <tr>
            <td><%= file.name %></td>
            <td><%= file.aasm_state %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
